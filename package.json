{
  "name": "resttemplate",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "node_modules/.bin/usdocker --refresh . -v --home ../db",
    "start-database": "node_modules/.bin/usdocker mysql up -v --home ../db --no-link",
    "stop-database": "node_modules/.bin/usdocker mysql down -v --home ../db"
  },
  "devDependencies": {
    "@usdocker/mysql": "^1.0.8",
    "@usdocker/usdocker": "^1.1.4"
  }
}
