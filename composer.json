{
  "name": "byjg/resttemplate",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "byjg/config": "2.0.*",
    "byjg/micro-orm": "2.2.0.x-dev",
    "byjg/authuser": "2.0.x-dev",
    "byjg/restserver": "2.1.*",
    "byjg/jwt-wrapper": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "RestTemplate\\": "src/project/",
      "Builder\\": "src/builder/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "migrate": "Builder\\Scripts::migrate",
    "build": "Builder\\Scripts::build",
    "restdocs": "Builder\\Scripts::genRestDocs",
    "post-create-project-cmd": "Builder\\PostCreateScript::run"
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "require-dev": {
    "zircote/swagger-php": "dev-master#06cc245f82982b68ff82d6259b49c67b3aef9a71 as 2.0.13",
    "byjg/swagger-test": "1.2.*",
    "byjg/migration": "2.0.*",
    "phpunit/phpunit": "6.5.*|5.7.*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/byjg/swagger-php"
    }
  ]
}
