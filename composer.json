{
  "name": "byjg/resttemplate",
  "description": "Boilerplate project template for create RESTFul services with docker and database integrated",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "php": ">5.6.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "byjg/config": "dev-bump",
    "byjg/micro-orm": "dev-bump",
    "byjg/authuser": "dev-bump",
    "byjg/mailwrapper": "dev-bump",
    "byjg/restserver": "dev-bump",
    "zircote/swagger-php": "^4.6.1",
    "byjg/swagger-test": "dev-bump",
    "byjg/migration": "dev-bump",
    "byjg/php-daemonize": "dev-bump",
    "byjg/shortid": "dev-bump",
    "byjg/jinja_php": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*|^9.5"
  },
  "autoload": {
    "psr-4": {
      "RestTemplate\\": "src/",
      "Builder\\": "builder/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "migrate": "Builder\\Scripts::migrate",
    "codegen": "Builder\\Scripts::codeGenerator",
    "openapi": "Builder\\Scripts::genOpenApiDocs",
    "post-create-project-cmd": "Builder\\PostCreateScript::run"
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
