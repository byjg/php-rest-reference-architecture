{
  "name": "byjg/resttemplate",
  "description": "Boilerplate project template for create RESTFul services with docker and database integrated",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "byjg/config": "3.0.*",
    "byjg/micro-orm": "2.2.*",
    "byjg/authuser": "3.0.*",
    "byjg/restserver": "3.0.*",
    "byjg/jwt-wrapper": "1.0.*",
    "zircote/swagger-php": "dev-master#941f0b62b7a2f2d784d5de13b146682e65449f53 as 2.0.13",
    "byjg/swagger-test": "1.2.*",
    "byjg/migration": "2.0.*"
  },
  "autoload": {
    "psr-4": {
      "RestTemplate\\": "src/project/",
      "Builder\\": "src/builder/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "migrate": "Builder\\Scripts::migrate",
    "build": "Builder\\Scripts::build",
    "restdocs": "Builder\\Scripts::genRestDocs",
    "post-create-project-cmd": "Builder\\PostCreateScript::run"
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">5.7"
  }
}
