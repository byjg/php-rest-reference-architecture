{
  "name": "byjg/resttemplate",
  "description": "Boilerplate project template for create RESTFul services with docker and database integrated",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "php": ">5.6.0",
    "ext-json": "*",
    "byjg/config": "3.0.*",
    "byjg/micro-orm": "4.0.*",
    "byjg/authuser": "4.0.*",
    "byjg/restserver": "3.0.*",
    "byjg/jwt-wrapper": "1.0.*",
    "zircote/swagger-php": "2.0.13.1",
    "byjg/swagger-test": "1.2.*",
    "byjg/migration": "4.0.*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/byjg/swagger-php"
    }
  ],
  "autoload": {
    "psr-4": {
      "RestTemplate\\": "src/project/",
      "Builder\\": "src/builder/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "migrate": "Builder\\Scripts::migrate",
    "docker-build": "Builder\\Scripts::dockerBuild",
    "docker-run": "Builder\\Scripts::dockerRun",
    "restdocs": "Builder\\Scripts::genRestDocs",
    "post-create-project-cmd": "Builder\\PostCreateScript::run"
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">5.7"
  }
}
